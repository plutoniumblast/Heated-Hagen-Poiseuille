FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
//Ubar 		Ubar [0 1 -1 0 0 0 0] (2 0 0)

transportModel Newtonian;

// Laminar viscosity
nu              1e-02;

// Thermal expansion coefficient
beta            3e-03;

// Reference temperature
TRef            273;

// Laminar Prandtl number
Pr              0.7;

// Turbulent Prandtl number
Prt             0.7;


